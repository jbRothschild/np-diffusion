@article{Tubiana2018,
abstract = {Statistical analysis of evolutionary-related protein sequences provides insights about their structure, function, and history. We show that Restricted Boltzmann Machines (RBM), designed to learn complex high-dimensional data and their statistical features, can efficiently model protein families from sequence information. We apply RBM to two protein domains, Kunitz and WW, and to synthetic lattice proteins for benchmarking. The features inferred by the RBM can be biologically interpreted in terms of structural modes, including residue-residue tertiary contacts and extended secondary motifs ({\$}\backslashalpha{\$}-helix and {\$}\backslashbeta{\$}-sheet), of functional modes controlling activity and ligand specificity, or of phylogenetic identity. In addition, we use RBM to design new protein sequences with putative properties by composing and turning up or down the different modes at will. Our work therefore shows that RBM are a versatile and practical tool to unveil and exploit the genotype-phenotype relationship for protein families.},
archivePrefix = {arXiv},
arxivId = {1803.08718},
author = {Tubiana, J{\'{e}}r{\^{o}}me and Cocco, Simona and Monasson, R{\'{e}}mi},
eprint = {1803.08718},
file = {:home/jrothschild/Research/Papers to read/Monasson{\_}Learning-Protein-Motifs{\_}2018.pdf:pdf},
pages = {1--19},
title = {{Learning protein constitutive motifs from sequence data}},
url = {http://arxiv.org/abs/1803.08718},
year = {2018}
}
@article{Ali2017,
abstract = {{\textcopyright} 2017 The Author(s). Published by Informa UK Limited, trading as Taylor {\&} Francis Group.Clustered regularly interspaced short palindromic repeats (CRISPR), linked with CRISPR associated (Cas) genes, can confer adaptive immunity to bacteria, against bacteriophage infections. Thus from a therapeutic standpoint, CRISPR immunity increases biofilm resistance to phage therapy. Recently, however, CRISPR-Cas genes have been implicated in reducing biofilm formation in lysogenized cells. Thus CRISPR immunity can have complex effects on phage–host–lysogen interactions, particularly in a biofilm. In this contribution, we develop and analyse a series of dynamical systems to elucidate and disentangle these interactions. Two competition models are used to study the effects of lysogens (first model) and CRISPR-immune bacteria (second model) in the biofilm. In the third model, the effect of delivering lysogens to a CRISPR-immune biofilm is investigated. Using standard analyses of equilibria, stability and bifurcations, our models predict that lysogens may be able to displace CRISPR-immune bacteria in a biofilm, and thus suggest strategies to eliminate phage-resistant biofilms.},
