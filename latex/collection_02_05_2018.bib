pmid = {27646134},
publisher = {Beijing Institute of Genomics, Chinese Academy of Sciences and Genetics Society of China},
title = {{Oxford Nanopore MinION Sequencing and Genome Assembly}},
url = {http://dx.doi.org/10.1016/j.gpb.2016.05.004},
