file = {:home/jrothschild/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Carmody et al. - 2015 - The daily dynamics of cystic fibrosis airway microbiota during clinical stability and at exacerbation.pdf:pdf},
isbn = {2049-2618 (Electronic)$\backslash$r2049-2618 (Linking)},
issn = {2049-2618},
journal = {Microbiome},
keywords = {airway microbiome,cystic fibrosis,lung microbiome,respiratory exacerbation},
number = {1},
pages = {12},
pmid = {25834733},
title = {{The daily dynamics of cystic fibrosis airway microbiota during clinical stability and at exacerbation}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=4381400{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {3},
